{
  "name": "buddy-exchange",
  "version": "1.0.0",
  "description": "Single page application realising a 'give one get one' exchange for CODECHECKs",
  "main": "index.html",
  "scripts": {
    "start": "python3 -m http.server 8000",
    "serve": "python3 -m http.server 8000",
    "download-deps": "npm run download-jquery && npm run download-bootstrap && npm run download-marked",
    "download-jquery": "curl -o assets/js/jquery.min.js https://code.jquery.com/jquery-3.7.1.min.js",
    "download-bootstrap": "curl -o assets/css/bootstrap.min.css https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css && curl -o assets/js/bootstrap.min.js https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js",
    "download-marked": "curl -o assets/js/marked.min.js https://cdn.jsdelivr.net/npm/marked@9.1.6/marked.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codecheckers/buddy-exchange.git"
  },
  "keywords": [
    "codecheck",
    "peer-review",
    "github",
    "javascript",
    "bootstrap",
    "jquery"
  ],
  "author": "CODECHECK Community",
  "license": "MIT",
  "homepage": "https://codecheckers.github.io/buddy-exchange/",
  "devDependencies": {},
  "dependencies": {},
  "engines": {
    "node": ">=14.0.0"
  }
}